{
  "name": "@routelink/client",
  "private": true,
  "version": "1.5.0",
  "type": "module",
  "scripts": {
    "postinstall": "npx husky install",
    "start": "vite --mode development",
    "test:dev": "vitest",
    "test": "vitest run",
    "build": "tsc && vite build",
    "prettier": "prettier -w .",
    "eslint": "eslint --fix .",
    "typecheck": "tsc --noEmit --strict --incremental false --tsBuildInfoFile null --project tsconfig.json",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "^31.3.2",
    "@ag-grid-community/core": "^31.2.2",
    "@ag-grid-community/styles": "^31.3.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.15.18",
    "@mui/material": "^5.15.18",
    "@mui/x-charts": "^7.5.1",
    "@mui/x-data-grid": "^7.5.1",
    "@mui/x-date-pickers": "^7.5.1",
    "@pbe/react-yandex-maps": "^1.2.5",
    "ag-grid-react": "^31.3.2",
    "axios": "^1.7.2",
    "cesium": "^1.117.0",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.1",
    "dotenv": "^16.4.5",
    "jwt-decode": "^4.0.0",
    "mobx": "^6.12.0",
    "mobx-react": "^9.1.1",
    "mobx-react-lite": "^4.0.6",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-helmet": "^6.1.0",
    "react-imask": "^7.6.1",
    "react-material-ui-carousel": "^3.4.2",
    "react-router-dom": "^6.23.1",
    "resium": "^1.17.3",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@taiga-ui/commitlint-config": "^0.7.5",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-unused-imports": "^3.2.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "sass": "^1.77.0",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-cesium": "^1.2.22",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.6.0"
  },
  "commitlint": {
    "extends": [
      "@taiga-ui/commitlint-config"
    ]
  },
  "lint-staged": {
    "*.{yml,js,ts,jsx,tsx,html,md,less,scss,css,json}": [
      "prettier -w"
    ]
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "deprecate",
        "section": "Deprecations"
      }
    ]
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.18.0",
    "@rollup/rollup-win32-x64-msvc": "^4.18.0"
  }
}
