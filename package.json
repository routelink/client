{
    "name": "@routelink/client",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "postinstall": "npx husky install",
        "start": "vite --port 3000",
        "test:dev": "vitest",
        "test": "vitest run",
        "build": "tsc && vite build",
        "prettier": "prettier !package-lock.json . --ignore-path .gitignore",
        "typecheck": "tsc --noEmit --strict --incremental false --tsBuildInfoFile null --project tsconfig.json",
        "prepare": "husky"
    },
    "dependencies": {
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@fontsource/roboto": "^5.0.12",
        "@mui/icons-material": "^5.15.14",
        "@mui/material": "^5.15.14",
        "axios": "^1.6.8",
        "dotenv": "^16.4.5",
        "jwt-decode": "^4.0.0",
        "mobx": "^6.12.0",
        "mobx-react-lite": "^4.0.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.22.3"
    },
    "devDependencies": {
        "@taiga-ui/commitlint-config": "^0.7.4",
        "@testing-library/jest-dom": "^6.4.2",
        "@testing-library/react": "^14.2.2",
        "@testing-library/user-event": "^14.5.2",
        "@types/react": "^18.2.73",
        "@types/react-dom": "^18.2.23",
        "@typescript-eslint/eslint-plugin": "^7.4.0",
        "@typescript-eslint/parser": "^7.4.0",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitest/ui": "^1.4.0",
        "eslint": "^8.56.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "husky": "^9.0.11",
        "jsdom": "^24.0.0",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "sass": "^1.71.1",
        "standard-version": "^9.5.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.2.2",
        "vite": "^5.1.4",
        "vite-tsconfig-paths": "^4.3.1",
        "vitest": "^1.4.0"
    },
    "commitlint": {
        "extends": [
            "@taiga-ui/commitlint-config"
        ]
    },
    "lint-staged": {
        "*.{yml,js,ts,html,md,less,scss,css,json}": [
            "prettier -w"
        ]
    },
    "standard-version": {
        "types": [
            {
                "type": "feat",
                "section": "Features"
            },
            {
                "type": "fix",
                "section": "Bug Fixes"
            },
            {
                "type": "deprecate",
                "section": "Deprecations"
            }
        ]
    }
}
